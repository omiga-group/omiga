    "@fontsource/roboto": "^4.5.8",
    "@manaflair/redux-batch": "^1.0.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "@reduxjs/toolkit": "^1.9.1",
    "clsx": "^1.2.1",
    "cuid": "^2.1.8",
    "final-form": "^4.20.7",
    "graphql-relay": "^0.10.0",
    "history": "^5.3.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "immutable": "^4.1.0",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.4.4",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.1",
    "relay-runtime": "^14.1.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-relay": "^14.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/relay-runtime": "^14.1.5",
    "@types/webpack-env": "^1.18.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-relay": "^14.1.0",
    "chance": "^1.1.9",
    "concat": "^1.0.3",
    "coveralls": "^3.1.1",
    "graphql": "^16.6.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^2.8.0",
    "react-scripts": "^5.0.1",
    "relay-compiler": "^14.1.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-config": "^12.0.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "start": "./scripts/env.sh && cp env-config.js ./public/ && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:ci": "CI=true react-scripts test --coverage --coverageReporters=text-lcov | coveralls",
    "eject": "react-scripts eject",
    "relay": "yarn run relay-compiler $@",
    "relay:watch": "yarn relay --watch",
    "lint": "eslint src",
    "fixlint": "eslint --fix --ext .js --ext .jsx --ext .ts --ext .tsx src",
    "format": "prettier --write --single-quote true --trailing-comma all --print-width 150 ./src && yarn fixlint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
